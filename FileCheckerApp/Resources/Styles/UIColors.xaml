<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--FONT COLORS-->
    <SolidColorBrush x:Key="textLight" Color="#ffffff"/>
    <SolidColorBrush x:Key="textDark" Color="#1e1e1e"/>

    <!--PANEL COLORS-->
    <SolidColorBrush x:Key="panelColor" Color="#ffffff"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#9dbede"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#5b8cb4"/>
    <SolidColorBrush x:Key="panelDisableColor" Color="#ffff"/>

    <SolidColorBrush x:Key="PrimaryButtonBackground" Color="#9dbede"/>
    <SolidColorBrush x:Key="PrimaryButtonForeground" Color="#1e1e1e"/>
    <SolidColorBrush x:Key="PrimaryButtonHoverBackground" Color="#9da8de"/>

</ResourceDictionary>